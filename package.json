{
  "name": "asign",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "new:build": "turbo run build --force",
    "test": "turbo run test",
    "schema": "turbo run schema --cache-dir=.turbo",
    "ci:publish": "turbo run build --filter=!docs && pnpm publish --no-git-checks -F \"@asunajs/*\"",
    "ci:version": "changeset version && pnpm install",
    "ci:pub:dist": "tsx build/mv-dist.ts && cd dist && pnpm publish --no-git-checks",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:preview": "pnpm --filter docs preview",
    "docs:start": "pnpm --filter docs start",
    "format": "dprint fmt",
    "lint": "oxlint -c ./eslintrc.json --fix",
    "prepare": "husky",
    "changeset": "changeset"
  },
  "keywords": [
    "sign",
    "airscript",
    "wps"
  ],
  "author": "catlair",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.5",
    "@types/node": "^20.14.8",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.5",
    "dprint": "^0.46.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "oxlint": "^0.4.4",
    "tsup": "^8.1.0",
    "tsx": "^4.15.7",
    "turbo": "^2.0.4",
    "typescript": "^5.5.2"
  },
  "private": true,
  "packageManager": "pnpm@9.0.4",
  "workspaces": ["packages/*", "core/*", "apps/*", "wps/*", "tools/*", "build", "docs"]
}
