{
  "name": "asign",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "new:build": "turbo run build --force",
    "test": "turbo run test",
    "schema": "turbo run schema --cache-dir=.turbo",
    "ci:publish": "turbo run build --filter=!docs && pnpm publish --no-git-checks -F \"@asunajs/*\"",
    "ci:version": "changeset version",
    "ci:pub:dist": "tsx build/mv-dist.ts && cd dist && pnpm publish --no-git-checks",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:preview": "pnpm --filter docs preview",
    "docs:start": "pnpm --filter docs start",
    "format": "dprint fmt",
    "lint": "oxlint -c ./eslintrc.json --fix",
    "prepare": "husky",
    "changeset": "changeset"
  },
  "keywords": [
    "sign",
    "airscript",
    "wps"
  ],
  "author": "catlair",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11",
    "@microsoft/api-extractor": "^7.48.1",
    "@types/node": "^20.17.11",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "dprint": "^0.46.3",
    "fastify-cli": "^7.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "oxlint": "^0.4.4",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "private": true,
  "packageManager": "pnpm@9.0.4",
  "workspaces": [
    "packages/*",
    "core/*",
    "apps/*",
    "wps/*",
    "tools/*",
    "build",
    "docs"
  ]
}
