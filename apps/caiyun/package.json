{
  "name": "@asunajs/caiyun",
  "version": "0.5.1",
  "description": "移动云盘签到",
  "main": "./index.ts",
  "type": "module",
  "types": "./index.d.ts",
  "bin": {
    "caiyun": "./cli.js",
    "cyc": "./cli2.js"
  },
  "exports": {
    ".": {
      "import": "./index.ts"
    }
  },
  "scripts": {
    "prepublish": "copyfiles package.json README.md dist/",
    "build": "tsup"
  },
  "keywords": [
    "sign",
    "airscript",
    "wps",
    "caiyun",
    "10086"
  ],
  "author": "catlair",
  "license": "MIT",
  "dependencies": {
    "@asunajs/conf": "workspace:^",
    "@asunajs/http": "workspace:^",
    "@asunajs/push": "workspace:^",
    "citty": "^0.1.6",
    "defu": "^6.1.4",
    "pm2": "^5.4.2"
  },
  "devDependencies": {
    "@asign/unstorage": "workspace:^",
    "@asign/build": "workspace:^",
    "@asign/caiyun-core": "workspace:^",
    "@asign/types": "workspace:^",
    "@asign/utils-pure": "workspace:^",
    "@asunajs/utils": "workspace:^",
    "compare-versions": "^6.1.1",
    "dotenv": "^16.4.5",
    "es-toolkit": "^1.27.0",
    "tsconfig": "workspace:*"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      ".": {
        "import": "./index.mjs",
        "types": "./index.d.ts",
        "require": "./index.cjs"
      }
    },
    "main": "./index.cjs",
    "types": "./index.d.cts",
    "module": "./index.mjs",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "url": "git+https://github.com/asunajs/asign.git"
  },
  "bugs": {
    "url": "https://github.com/asunajs/asign/issues"
  },
  "engines": {
    "node": ">= 14.12.1"
  },
  "homepage": "https://as.js.cool",
  "private": true
}
