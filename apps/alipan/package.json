{
  "name": "@asunajs/alipan",
  "version": "0.1.6",
  "type": "module",
  "description": "阿里云盘签到",
  "main": "index.ts",
  "types": "index.d.ts",
  "bin": "cli.js",
  "scripts": {
    "prepublish": "copyfiles package.json README.md dist/",
    "build": "tsup"
  },
  "keywords": [
    "sign",
    "airscript",
    "wps",
    "alipan"
  ],
  "author": "catlair",
  "license": "MIT",
  "dependencies": {
    "@asunajs/conf": "workspace:^",
    "@asunajs/http": "workspace:^",
    "@asunajs/push": "workspace:^",
    "secp256k1": "^5.0.1"
  },
  "devDependencies": {
    "@asign/alipan-core": "workspace:^",
    "@asign/build": "workspace:^",
    "@asign/utils-pure": "workspace:^",
    "@asunajs/utils": "workspace:^",
    "@types/secp256k1": "^4.0.6",
    "tsconfig": "workspace:*"
  },
  "publishConfig": {
    "directory": "dist",
    "linkDirectory": false,
    "exports": {
      ".": {
        "import": "./index.mjs",
        "types": "./index.d.ts",
        "require": "./index.cjs"
      }
    },
    "main": "./index.cjs",
    "types": "./index.d.cts",
    "module": "./index.mjs",
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "url": "git+https://github.com/asunajs/asign.git"
  },
  "bugs": {
    "url": "https://github.com/asunajs/asign/issues"
  },
  "engines": {
    "node": ">= 14.12.1"
  },
  "homepage": "https://as.js.cool"
}
